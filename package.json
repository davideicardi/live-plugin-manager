    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.1",
    "@types/node": "^10.3.2",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "source-map-support": "^0.5.6",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
