  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "source-map-support": "^0.5.21",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/lockfile": "^1.0.2",
