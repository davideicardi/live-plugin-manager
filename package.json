  "devDependencies": {
    "@types/chai": "^4.3.14",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.18.96",
    "chai": "^4.4.1",
    "mocha": "^9.2.2",
    "source-map-support": "^0.5.21",
    "typescript": "^4.9.5"
  },
  "dependencies": {
